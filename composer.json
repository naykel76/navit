{
    "name": "naykel/navit",
    "description": "Navigation and page management package for NayKel Laravel Applications",
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable" : true,
    "authors": [
        {
            "name": "Nathan Watts",
            "email": "nathan@naykel.com.au",
            "homepage": "https://naykel.com.au"
        }
    ],
    "homepage": "https://github.com/naykel76/navit.git",
    "keywords": ["Laravel", "Navit"],
    "require": {
        "illuminate/support": "~7|~8"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "orchestra/testbench": "~5|~6"
    },
    "autoload": {
        "psr-4": {
            "Naykel\\Navit\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Naykel\\Navit\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        },
        "laravel": {
            "providers": [
                "Naykel\\Navit\\NavitServiceProvider"
            ]
        }
    }
}
